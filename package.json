  "scripts": {
    "postmerge": "yarn install",
    "postinstall": "yarn list --depth=0 | grep @gov.au",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "prebuild": "del public/images/**/* && del public/javascripts/**/* && del public/stylesheets/**/*",
    "build:stats": "webpack --profile --json > webpackstats.json",
    "build": "NODE_ENV=production webpack -p --release",
    "start": "node client/scripts/start.js",
    "test": "npm run test-browser && npm run test-browser:legacy && npm run test:react",
    "test:lint": "eslint client/src/editor",
    "test:react": "NODE_ENV=test jest --env=jsdom",
    "test:react:watch": "npm run test:react -- --watch --verbose",
    "test:react:coverage": "npm run test:react -- --coverage --runInBand",
    "test-browser": "NODE_ENV=test ava 'client/test/**/*.test.js' --require=babel-register",
    "test-browser:tap": "NODE_ENV=test npm run test-browser -- --tap | tap-spec",
    "test-browser:watch": "NODE_ENV=test DEBUG=ava:watcher ava 'client/test/**/*.test.js' --require=babel-register --watch --verbose",
    "test-browser:legacy": "NODE_ENV=test karma start",
    "storybook": "start-storybook -p 6006 -c client/.storybook",
    "build-storybook": "build-storybook -c client/.storybook -o client/.out",
    "deploy-storybook": "npm run build-storybook && surge -p client/.out -d performance-dashboard-ui.surge.sh"
  },
  "engines": {
    "node": ">=8.9.3"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/client/config/jest/preprocessor.js",
    "setupFiles": [
      "<rootDir>/client/config/polyfills.js"
    ],
    "testPathDirs": [
      "<rootDir>/client/src/"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "testPathIgnorePatterns": [
      "<rootDir>/client/src/editor/scripts/__tests__/fixtures"
    ],
    "testURL": "http://localhost",
    "coverageDirectory": "client/coverage",
    "collectCoverageFrom": [
      "client/src/**/*.js",
      "!**/node_modules/**",
      "!**/*/index.js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "client/src/editor/scripts/_fixtures",
      "client/src/test",
      "client/src/dashboard",
      "client/src/login",
      "client/src/app_shell",
      "client/src/_common",
      "client/src/_shared",
      "client/src/_storybook"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/client/config/jest/CSSStub.js"
    },
    "globals": {
      "__DEV__": true
    }
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kadira/storybook-addon-knobs": "^1.6.0",
    "babel-plugin-module-resolver": "^2.7.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "codecov": "^1.0.1",
    "commitizen": "^2.9.1",
    "cz-conventional-changelog": "^1.2.0",
    "react-dev-utils": "^0.4.2",
    "strip-json-comments": "^2.0.1",
    "surge": "^0.18.0",
    "validate-commit-msg": "^2.8.2",
    "webpack-bundle-analyzer": "^2.3.0",
    "husky": "^0.12.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "helpMessage": "\nPlease fix your commit message (and consider using http://npm.im/commitizen)\n",
      "warnOnFail": false
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
